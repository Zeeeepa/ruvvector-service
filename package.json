{
  "name": "ruvvector-service",
  "version": "1.0.0",
  "description": "Thin, stateless API service for RuvVector operations",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "vector",
    "api",
    "ruvvector"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "claude-flow": "^2.7.47",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pino": "^8.16.2",
    "pino-http": "^8.5.1",
    "prom-client": "^15.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
